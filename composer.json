{
    "name": "laravel-notification-channels/africastalking",
    "description": "Laravel notification channel driver for AfricasTalking",
    "homepage": "https://github.com/laravel-notification-channels/africastalking",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Maiyo",
            "email": "kipropbrian@gmail.com",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AfricasTalking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AfricasTalking\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
